{
    "command": "config-set",
    "service": ["dhcp4"],
    "arguments":  {
        
        "Dhcp4": {
                "authoritative": true,
                "interfaces-config": {
                    "interfaces": [ "eth0" ]
                },
                "control-socket": {
                    "socket-name": "/tmp/kea4-ctrl-socket",
                    "socket-type": "unix"
                },
                "loggers": [
                    {"name": "kea-dhcp4","severity": "DEBUG","output_options": [{"output": "/usr/local/var/log/kea-dhcp4.log"}]}
                    ],
                "subnet4": [
                {
                    "subnet": "172.18.0.0/16",
                    "pools": [
                        {
                            "pool": "172.18.20.0 - 172.18.30.0"
                        }
                    ],
                    "option-data": [
                        {"name": "routers","data":"172.18.255.254"}
                    ],
                    "reservations": [
                        {"hw-address": "1a:1b:1c:1d:1e:1f","ip-address":"172.18.40.1","hostname": "special-snowflake"}
                    ]
                }
            ]
        }
    }
}